:start					; Start here
	mov %ax $1			; Set system interrupt
	mov %bx &myNum		; Set number to be cubed
	call @cube
	sysi

:square
	mul %bx %bx			; Square number
	ret

:cube
	push %bx			; Save original number
	call @square		; Square the number
	pop %cx				; Get original back
	mul %bx %cx			; Multiply once more
	ret

#data
	stb *myNum $5
#end